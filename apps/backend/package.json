{
  "name": "fiscus-backend",
  "version": "0.1.0",
  "description": "Fiscus financial backend API",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:generate": "TS_NODE_TRANSPILE_ONLY=true typeorm-ts-node-commonjs -d ./ormconfig.ts migration:generate",
    "migration:run": "TS_NODE_TRANSPILE_ONLY=true typeorm-ts-node-commonjs -d ./ormconfig.ts migration:run",
    "migration:revert": "TS_NODE_TRANSPILE_ONLY=true typeorm-ts-node-commonjs -d ./ormconfig.ts migration:revert",
    "seed": "TS_NODE_TRANSPILE_ONLY=true ts-node -r tsconfig-paths/register src/database/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.991.0",
    "@aws-sdk/client-sns": "^3.994.0",
    "@aws-sdk/credential-provider-ini": "^3.972.8",
    "@aws-sdk/property-provider": "^3.374.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-ws": "^11.0.1",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.1",
    "@sendgrid/mail": "^8.1.0",
    "@types/nodemailer": "^7.0.10",
    "amqplib": "^0.10.3",
    "aws-sdk": "^2.1500.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "minio": "^7.1.3",
    "nestjs-i18n": "^10.6.0",
    "nodemailer": "^8.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.10.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.12.2",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/amqplib": "^0.10.4",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/minio": "^7.0.18",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.9.0",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
